syntax on

set backspace=indent,eol,start
set cursorline
set expandtab
set foldenable
set hlsearch
set incsearch
set number
set showcmd
set showmatch
set softtabstop=4
set tabstop=4
set wildmenu

highlight LineNr ctermfg=black ctermbg=49
highlight EndOfBuffer ctermfg=black

inoremap jj <Esc>
inoremap <C-c> <Esc>

